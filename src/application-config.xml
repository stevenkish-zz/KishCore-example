<!-- mxmlc -file-specs=/Users/stevenkish/Workspace/kish/src/Application.as -->

<flex-config>
	<!--
	<output>${project.directory}/deploy/bin/application.swf</output>
	-->
	<target-player>10.0.1</target-player>
	<benchmark>true</benchmark>
	<default-background-color>#ffffff</default-background-color>
	<default-frame-rate>40</default-frame-rate>
	<default-size>
		<width>320</width>
		<height>480</height>
	</default-size>
	<compiler>
		<incremental>true</incremental>
		<optimize>true</optimize>
		<keep-generated-actionscript>false</keep-generated-actionscript>
		<warn-no-constructor>false</warn-no-constructor>
		<warn-no-explicit-super-call-in-constructor>false</warn-no-explicit-super-call-in-constructor>
		<show-actionscript-warnings>false</show-actionscript-warnings>
		<show-binding-warnings>true</show-binding-warnings>
		<show-unused-type-selector-warnings>true</show-unused-type-selector-warnings>
		<debug>true</debug>
	</compiler>
	<metadata>
		<title>Kish Core Application</title>
		<description>A lightweight ActionScript 3.0 framework for Flash Player 10</description>
		<creator>Steven Kish</creator>
		<language>EN</language>
	</metadata>
	<raw-metadata></raw-metadata>
</flex-config>
